var bubblefreqthing = function() {
	bubblemaker()
  bubblefreq=((2250*(air/75))+250)
  timeout = setTimeout(bubblefreqthing, bubblefreq);
}
var timeout = setTimeout(bubblefreqthing, bubblefreq);
var bubblemover = setInterval(function(){
bubbleexist = document.querySelectorAll('.bubble');
bubbleexist.forEach(existingelement => {
  bubbley=existingelement.offsetTop
  existingelement.style.top=(bubbley-1)+"px"
  bubbley=0
  clickHandlerbubble(existingelement)
})
}, 25)
var flying = setInterval(function(){
if (flighty==-1){
playery+=-1
}
if (flighty==1){
playery+=1
}
if (flightx==-1){
playerx+=-1
}
if (flightx==1){
playerx+=1
}
}, 1)
var breathe = setInterval(function(){
if (died==-1 && freezemost==0){
if (breathing==0 && gills==0 || fullyinsidewall==1 || breathing==1 && gills==1){
if (noairnoprob==0){
air=air-0.25
}
} else if (breathing==1 && air<75 && gills==0 || breathing==0 && air<75 && gills==1 || fullyinsidewall==1 && air<75){
if (air<0){
air=0
} else if (died==-1){
air=air+2.5
if (air>75){
air=75
}
}
}
} else {
if (air!=0 && died==1){
air=air-2.5
}
}
document.getElementById("airamount").style="z-index: 9999999; display: inline-block; position: fixed; height: 5px; overflow: hidden; width: 75px; top: 20px; left: "+(air-75)+"px; background-color: #008FFF"
}, 50)
function MobileDetectr(){
iOS(checksout)
if (checksout=="iPad" || checksout=="iPhone" || checksout=="iPod"){
mobile=1
} else {
mobile=0
}
}
function iOS() {
  return [
    'iPad Simulator',
    'iPhone Simulator',
    'iPod Simulator',
    'iPad',
    'iPhone',
    'iPod'
  ].includes(navigator.platform)
  // iPad on iOS 13 detection
  || (navigator.userAgent.includes("Mac") && "ontouchend" in document)
}
mobileshift.onpointerdown = function() {
	if (flight==1){
  flighty=1
  }
}
mobileshift.onpointerup = function() {
	if (flighty==1){
  flighty=0
  }
}
mobilejump.onpointerdown = function() {
	if (flight==1){
  flighty=-1
  }
}
mobilejump.onpointerup = function() {
	if (flighty==-1){
  flighty=0
  }
}
mobileleft.onpointerdown = function() {
	if (flight==0){
  moving=-1
  } else if (flightx!=1) {
  flightx=-1
  }
}
mobileleft.onpointerup = function() {
  moving=0
  if (flightx!=1){
  flightx=0
  }
}
mobileright.onpointerdown = function() {
	if (flight==0){
  moving=1
  } else if (flightx!=-1) {
  flightx=1
  }
}
mobileright.onpointerup = function() {
  moving=0
  if (flightx!=-1){
  flightx=0
  }
}
MobileDetectr()
var voiddamage = setInterval(function(){
if (died==-1 && voider==1 && freezemost==0){
if (abhp!=0){
abhp-=1
} else if (hp>1) {
hp-=1
healthcounter=-1000
} else {
hp-=1
healthcounter=-1000
died=1
}
}
}, 777)
var regen = setInterval(function(){
if (died==-1 && hp<12 && breathing==1 && canheal==1){
if (healthcounter>=250){
hp=hp+1
healthcounter-=250
} else {
healthcounter+=1
}
} else {
healthcounter=-1000
}
}, 10)
function spawndeadplayer(){
if (died==1 && spawns !== 0){
if (dead==0){
deadx=playerx
deady=playery
deadxchange=playerxchange
deadychange=playerychange
document.getElementById("deadplayer").opacity=0
dead=1
} else {
deadx=playerx
deady=playery
deadxchange=playerxchange
deadychange=playerychange
document.getElementById("deadplayer").opacity=0
}
}
}
